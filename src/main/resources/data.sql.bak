-- Reviews 삽입
INSERT INTO reviews (id, user_id, restaurant_id, content, rating, menu, price, visit_date, sympathy_count, is_first_review, created_at, updated_at) VALUES
(1, 1, 1, '40년 전통의 맛! 진한 국물에 순대도 신선하고 양도 푸짐합니다. 아침 출근 전에 먹기 딱 좋아요. 특히 막창이 들어간 모듬순대국 강추!', 5.0, '모듬순대국', '10,000원', '2024-12-01', 156, false, '2024-12-02 10:00:00', '2024-12-02 10:00:00'),
(2, 2, 3, '손으로 빚은 만두가 정말 맛있어요. 칼국수 면발도 쫄깃하고 국물이 깔끔해서 자주 갑니다. 점심시간에는 웨이팅이 있으니 참고하세요!', 4.0, '만두칼국수', '12,000원', '2024-11-28', 89, false, '2024-11-30 12:00:00', '2024-11-30 12:00:00'),
(3, 3, 5, '성수동에서 파스타 맛집 찾는다면 여기! 생면 파스타에 트러플 오일 향이 진하게 나요. 분위기도 좋아서 데이트 코스로 추천드립니다.', 5.0, '트러플 크림 파스타', '24,000원', '2024-12-03', 234, false, '2024-12-03 14:00:00', '2024-12-03 14:00:00'),
(4, 8, 7, '진한 돈코츠 육수에 챠슈가 입에서 녹아요. 면 익힘도 선택 가능하고 무한리필 김치도 맛있습니다. 라멘 좋아하시는 분들 강추!', 5.0, '특제 돈코츠라멘', '14,000원', '2024-12-01', 78, false, '2024-12-02 11:00:00', '2024-12-02 11:00:00'),
(5, 9, 9, '인천 차이나타운에서 짬뽕 맛집으로 유명한 곳! 얼큰한 국물에 해물이 푸짐하게 들어가요. 탕수육도 바삭바삭 찍먹파라면 여기!', 5.0, '삼선짬뽕', '12,000원', '2024-11-25', 145, false, '2024-11-26 13:00:00', '2024-11-26 13:00:00'),
(6, 4, 13, '익선동 한옥 카페! 인테리어가 정말 예쁘고 시그니처 한방차가 독특해요. 사진 찍기 좋고 조용해서 대화하기도 좋습니다.', 4.0, '오미자차', '7,000원', '2024-11-30', 67, false, '2024-12-01 15:00:00', '2024-12-01 15:00:00'),
(7, 6, 16, '식빵이 진짜 유명한 곳! 줄 서서 먹는 이유가 있어요. 갓 구운 식빵의 버터향이 정말 좋고 질감도 완벽합니다.', 5.0, '우유식빵', '8,000원', '2024-12-02', 198, false, '2024-12-03 09:00:00', '2024-12-03 09:00:00'),
(8, 7, 19, '신당동 떡볶이 골목의 원조집! 쫄깃한 밀떡에 달달매콤한 양념이 중독적이에요. 튀김이랑 순대도 같이 드시면 완벽!', 5.0, '떡볶이 세트', '9,000원', '2024-11-28', 112, false, '2024-11-29 18:00:00', '2024-11-29 18:00:00'),
(9, 12, 11, '드라이에이징 스테이크의 정점! 미디엄 레어로 구워주시는데 육즙이 장난 아니에요. 특별한 날 가기 딱 좋은 곳입니다.', 5.0, '드라이에이징 립아이', '85,000원', '2024-11-20', 89, false, '2024-11-21 19:00:00', '2024-11-21 19:00:00'),
(10, 11, 6, '평양냉면 입문자에게 추천! 담백한 육수에 쫄깃한 면발이 일품이에요. 수육도 부드럽고 겨자는 살짝만 넣는 게 포인트!', 4.0, '물냉면', '14,000원', '2024-12-01', 56, false, '2024-12-02 12:30:00', '2024-12-02 12:30:00');

